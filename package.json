{
  "name": "elderly-services-finder",
  "private": true,
  "version": "1.99.147",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"node server.js\" \"vite\"",
    "server": "node server.js",
    "client": "vite",
    "build": "vite build && node post-build.js",
    "build:netlify": "vite build && node post-build.js && node scripts/verify-build.js",
    "preview": "vite preview",
    "bump": "node build.js --bump",
    "push": "npm run bump && npm run build && git add . && git commit -m \"v$npm_package_version\" && git push",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "firebase": "^10.8.0",
    "firebase-admin": "^13.2.0",
    "fuse.js": "^7.0.0",
    "idb": "^8.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^4.0.1",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.1.2",
    "fake-indexeddb": "^6.1.0",
    "firebase-functions-test": "^3.4.1",
    "happy-dom": "^17.6.1",
    "supertest": "^7.1.4",
    "vite": "^5.0.8",
    "vitest": "^3.2.4"
  }
}
